<?xml version="1.0" encoding="utf-8"?>
<rotate
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:fromDegrees="90"
    android:toDegrees="90">
    <shape
        android:shape="line"
        android:top="1dp">
        <stroke
            android:color="#595959"
            android:width="1dp"/>
        <size
            android:height="5dp"
            android:width="120dp"/>
    </shape>
</rotate>